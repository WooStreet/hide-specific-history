{
  "manifest_version": 3,
  "name": "Hide Specific History",
  "version": "1.0.1",
  "description": "This is a chrome extension to keep certain URLs out of the history.",
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "assets/scripts/serviceWorker.js"
  },
  "permissions": [
    "history",
    "webNavigation",
    "tabs",
    "storage"
  ]
}